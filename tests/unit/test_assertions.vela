@test
fn testMathOperations() -> void {
  let result = 2 + 3
  assertEquals(result, 5, "2 + 3 should equal 5")

  let multiply = 4 * 3
  assertEquals(multiply, 12, "4 * 3 should equal 12")

  println("Math operations work correctly!")
}