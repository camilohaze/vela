import 'stdlib:test'

import 'stdlib:test'

fn test_basic_arithmetic() -> void {
    result: Number = 2 + 3
    assert(result == 5, "2 + 3 should equal 5")
}

fn test_string_concatenation() -> void {
    greeting: String = "Hello" + " " + "World"
    assert(greeting == "Hello World", "String concatenation should work")
}

// Ejecutar tests
run_test(test_basic_arithmetic, "test_basic_arithmetic")
run_test(test_string_concatenation, "test_string_concatenation")
report_results()