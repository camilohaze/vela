def add_numbers(a: int, b: int) -> int:
    """Add two numbers together."""
    return a + b

def multiply_numbers(a: int, b: int) -> int:
    """Multiply two numbers."""
    return a * b

# Module-level constant
PI = 3.14159

# Module-level function
def get_pi() -> float:
    """Get the value of PI."""
    return PI